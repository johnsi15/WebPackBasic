{
  "name": "webpackbasic",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --entry ./src/js/app.js --output-filename ./dist/bundle.js",
    "build": "webpack",
    "dev": "webpack -w",
    "prod": "webpack -p"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "commonjs": true,
      "es6": true,
      "node": true,
      "jquery": true
    },
    "parser": "babel-eslint",
    "rules": {
      "strict": 0,
      "no-console": "warn",
      "no-use-before-define": "warn",
      "spaced-comment": "warn"
    },
    "extends": "eslint:recommended"
  },
  "eslintIgnore": [
    "/node_modules/*",
    "build/*",
    "!build/index.js",
    "dist/"
  ],
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "css-loader": "^0.28.5",
    "eslint": "^3.19.0",
    "style-loader": "^0.18.2",
    "webpack": "^3.5.5"
  },
  "dependencies": {
    "jquery": "^3.2.1"
  }
}
